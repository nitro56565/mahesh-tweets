{
    "manifest_version": 3,
    "name": "Mahesh Tweets",
    "version": "1.0",
    "permissions": [
      "activeTab"
    ],
    "content_scripts": [
      {
        "js": ["content.js"]
      }
    ],
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "128": "/images/icon.png"
      }
    },
    "icons": {
      "128": "/images/icon.png"
    }
}